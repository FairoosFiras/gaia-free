# Frontend Environment Variables for Docker
# Copy this to .settings.docker.env and customize as needed

# Auth0 Configuration (public values - not secrets)
VITE_AUTH0_DOMAIN=dev-XXXX.us.auth0.com
VITE_AUTH0_CLIENT_ID=XXXX
VITE_AUTH0_AUDIENCE=https://XX.XXX.ai
VITE_REQUIRE_AUTH=true

# Debug mode
VITE_DEBUG=true

# Allowed hosts for Vite dev server (comma-separated)
VITE_ALLOWED_HOSTS=localhost,127.0.0.1
